      <ul class="navbar-nav ml-auto">
        <li class="nav-item dropdown">
          <a class="nav-link" data-toggle="dropdown" href="#">
            <span class="badge badge-pill badge-secondary p-2">
              Emulator&nbsp;
              <% if (emulator.enable) { %>
              <i class="fa fa-pause-circle"></i>
              <% } else { %>
              <i class="fa fa-play-circle"></i>
              <% } %>
            </span>
          </a>
          <div class="dropdown-menu dropdown-menu-lg dropdown-menu-right">
            <div class="card-body">
              <div class="row">
                <div class="col">
                  <label for="TimeInterval">Time Interval:</label>
                  <input type="number" class="form-control" id="TimeInterval" value="<%= emulator.interval %>"
                    <% if (emulator.enable) { %>disabled<% } %>>
                </div>
              </div>
              <div class="row mt-1">
                <div class="d-flex flex-row align-items-center">
                  <div class="p-2">
                    <label for="emulator">
                      <% if (emulator.enable) { %>
                      Disable:
                      <% } else { %>
                      Enable:
                      <% } %>
                    </label>
                  </div>
                  <div class="p-2">
                    <a id="emulator_url" class="nav-link h4" href="emulator/<%= emulator.interval %>"
                      aria-expanded="false" title="Emulator">
                      <% if (emulator.enable) { %>
                      <i id="emulator" class="fa fa-toggle-on"></i>
                      <% } else { %>
                      <i id="emulator" class="fa fa-toggle-off"></i>
                      <% } %>
                    </a>
                  </div>

                </div>
              </div>
            </div>
          </div>
